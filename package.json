{
  "name": "spacepack",
  "version": "0.0.1",
  "description": "js bundler",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava",
    "coverage": "nyc report --reporter=json && codecov -f coverage/coverage-final.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spacepack/spacepack.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spacepack/spacepack/issues"
  },
  "homepage": "https://github.com/spacepack/spacepack#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.0",
    "flow-bin": "^0.73.0",
    "nyc": "^11.6.0",
    "proxyquire": "^2.0.1",
    "sinon": "^5.0.0"
  },
  "dependencies": {
    "@most/create": "^2.0.1",
    "chokidar": "^2.0.2",
    "most": "^1.7.2"
  },
  "babel": {
    "presets": [
      "flow",
      "es2015"
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.js",
      "!coverage/**/*"
    ],
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "flow",
        "es2015",
        "@ava/stage-4",
        "@ava/transform-test-files"
      ]
    }
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  }
}
